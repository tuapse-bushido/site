{
  "name": "bushido",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack",
    "dev:local": "concurrently \"next dev --webpack\" \"tsx socket/server.ts\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=dot",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ],
    "socket/**/*.{js,ts}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.932.0",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.5",
    "@mui/material-nextjs": "^7.3.5",
    "@mui/x-data-grid": "^8.19.0",
    "@reduxjs/toolkit": "^2.10.1",
    "bcryptjs": "^3.0.3",
    "clsx": "^2.1.1",
    "jose": "^6.1.2",
    "next": "^16.0.5",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "react-select": "^5.10.2",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.8.1",
    "swiper": "^12.0.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/webpack": "^5.28.5",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.9",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "sass": "^1.94.0",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "typescript-eslint": "^8.47.0",
    "vitest": "^4.0.9"
  }
}
